{
  "extends": "./paths.json",
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "allowJs": true,
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "esModuleInterop": true,
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "strict": true,
    "strictNullChecks": true,
    "plugins": [
      {"transform": "typia/lib/transform", }
    ],
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "outDir": "dist"
  }

  
}